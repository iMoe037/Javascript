/*
 * Complete the function below.
 */
function palindrome(str) {
    var strArray = str.split('');
    var reverseStr = "";
    for(var i = strArray.length-1; i>=0; i--){
        reverseStr += str[i];
    }
    if(str.toLowerCase() === reverseStr.toLowerCase()){
        return true;
    }
    else  {
        return false;
    }
}
palindrome("Racecar");
